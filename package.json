{
  "name": "kfc",
  "version": "1.0.0",
  "description": "A web based kafka UI",
  "main": "index.js",
  "scripts": {
    "start": "concurrently webpack \"tsc -w -p ./server/tsconfig.json\" \"nodemon ./dist/server/\"",
    "test": "npm run lint",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build:dev": "webpack && tsc -p ./server/tsconfig.json",
    "build:prod": "npm run lint && webpack --config ./webpack.config.prod.js && tsc -p ./server/tsconfig.json --sourceMap false",
    "build:serveronly": "tsc -p ./server/tsconfig.json"
  },
  "keywords": [
    "kafka",
    "administration",
    "react"
  ],
  "author": "Jeppe Andersen",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.4",
    "kafka-node": "^3.0.1",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.12",
    "@types/query-string": "^6.1.1",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-icons": "^2.2.6",
    "@types/react-redux": "^6.0.10",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/socket.io": "^2.1.1",
    "@types/socket.io-client": "^1.4.32",
    "@types/webpack": "^4.4.20",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.4",
    "autoprefixer": "^9.4.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.18.7",
    "postcss": "^7.0.6",
    "postcss-loader": "^3.0.0",
    "query-string": "^6.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-icons": "^3.2.2",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "socket.io-client": "^2.2.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-serve": "^2.0.3"
  }
}
